<?xml version="1.0" encoding="utf-8"?>
<CustomStickyScrollView.StickyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:whatever="http://schemas.android.com/tools"
    android:id="@+id/Nested_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="25dp"
    android:visibility="visible"
    whatever:stuckShadowDrawable="@drawable/sticky_shadow_default">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:gravity="center"
            android:tag="sticky"
            android:text=" Main Rolet" />

        <Button
            android:id="@+id/Rolet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Rolet" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:tag="sticky"
            android:gravity="center"
            android:text=" Menu items visibility" />

        <Button
            android:id="@+id/VisibileAll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="VISIBIL ALL" />

        <Button
            android:id="@+id/HideAll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="HIDE ALL" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:tag="sticky"
            android:gravity="center"
            android:text=" Upload" />

        <Button
            android:id="@+id/Uploadvideogallery"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="video gallery" />

        <Button
            android:id="@+id/Uploadvideocamera"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="video camera" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:tag="sticky"
            android:gravity="center"
            android:text=" Media Player" />

        <Button
            android:id="@+id/MediaPlayer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Media Player" />

        <Button
            android:id="@+id/MediaPlayerFullScreen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Media Player FullScreen" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:tag="sticky"
            android:gravity="center"
            android:text=" fragment slider main" />

        <Button
            android:id="@+id/fragment_slider_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="fragment in fragment slider main" />

        <Button
            android:id="@+id/fragment_slider_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="fragment slider main" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="#0dd"
                android:gravity="center"
                android:tag="sticky"
                android:text="RecyclerView" />

            <Button
                android:id="@+id/RecyclerView_Style1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_1\n(Snapping RecyclerView)" />

            <Button
                android:id="@+id/RecyclerView_Style2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_2\n(Bottom To Top)" />

            <Button
                android:id="@+id/RecyclerView_Style3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_2\n(Switching Bottom To Top)" />

            <Button
                android:id="@+id/RecyclerView_Style4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_2\n(Switching Top To Bottom)" />

            <Button
                android:id="@+id/RecyclerView_Style5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_5\n(Center)" />

            <Button
                android:id="@+id/RecyclerView_Style6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_6\n(both)" />

            <Button
                android:id="@+id/RecyclerView_Style7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_7\n(both 2)" />

            <Button
                android:id="@+id/RecyclerView_Style8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="RecyclerView Style_8\n(both 3)" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="#0dd"
                android:tag="sticky"
                android:gravity="center"
                android:text="Permission" />

            <Button
                android:id="@+id/Requesting_Single_Permission"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Requesting Single Permission" />

            <Button
                android:id="@+id/Requesting_Multiple_Permission"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Requesting Multiple Permissions" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="#0dd"
                android:tag="sticky"
                android:gravity="center"
                android:text="slider" />

            <Button
                android:id="@+id/Button_slider_style1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="slider animation style1" />

            <Button
                android:id="@+id/Button_slider_style2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="slider animation style2" />

            <Button
                android:id="@+id/Button_slider_style3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="slider animation style3" />

            <Button
                android:id="@+id/Button_slider_style4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="slider animation style4" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="56dp"
                android:background="#0dd"
                android:tag="sticky"
                android:gravity="center"
                android:text="Custom_MessageBox" />

            <Button
                android:id="@+id/custommessagebox_style1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="custom messagebox style1" />

            <Button
                android:id="@+id/custommessagebox_style2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="custom messagebox style2" />

            <Button
                android:id="@+id/custommessagebox_style3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="custom messagebox style3" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:gravity="center"
            android:tag="sticky"
            android:text="only_text" />

        <Button
            android:id="@+id/doNoting_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Do Noting Notification" />

        <Button
            android:id="@+id/openApplication_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open Application Notification" />

        <Button
            android:id="@+id/showsite_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open site Notification" />

        <Button
            android:id="@+id/showSpacialActivity_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Show Special Activity Notification" />

        <Button
            android:id="@+id/openPhoneDialort_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open Phone Dialer Notification" />

        <Button
            android:id="@+id/SendSms_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open SendSms Notification" />

        <Button
            android:id="@+id/email_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open Email notification" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="#0dd"
            android:tag="sticky"
            android:gravity="center"
            android:text="image_text" />

        <Button
            android:id="@+id/Image_doNoting_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Do Noting Notification" />

        <Button
            android:id="@+id/Image_openApplication_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Open Application Notification" />

        <Button
            android:id="@+id/Image_showsite_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Open site Notification" />

        <Button
            android:id="@+id/Image_showSpacialActivity_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Show Special Activity Notification" />

        <Button
            android:id="@+id/Image_openPhoneDialort_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Open Phone Dialer Notification" />

        <Button
            android:id="@+id/Image_SendSms_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Open SendSms Notification" />

        <Button
            android:id="@+id/Image_email_notification"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Image Open Email notification" />
    </LinearLayout>



</CustomStickyScrollView.StickyScrollView>